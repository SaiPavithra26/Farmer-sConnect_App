{
  "expo": {
    "name": "FarmersConnect",
    "slug": "farmersconnect",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "farmersconnect",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourcompany.farmersconnect"
    },
    "android": {
      "package": "com.yourcompany.farmersconnect",
      "permissions": [
        "CAMERA",
        "LOCATION",
        "NOTIFICATIONS"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      "expo-localization",
      "expo-notifications",
      "expo-camera",
      "expo-location",
      "expo-image-picker",
      "expo-file-system",
      "expo-media-library",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}